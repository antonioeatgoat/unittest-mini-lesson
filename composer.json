{
  "name": "antonioeatgoat/unittest-mini-lesson",
  "description": "A small practical lesson to unittest applied to WordPress",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Antonio Mangiacapra",
      "email": "info@webbaku.com"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "vendor/bin/phpunit",
    "coverage": "vendor/bin/phpunit --coverage-html tests/coverage"
  },
  "require-dev": {
    "10up/wp_mock": "dev-dev",
    "phpunit/phpunit": "^6.5"
  },
  "require": {
    "php": "^7.0.0"
  },
  "autoload": {
    "psr-4": {
      "AntonioEatGoat\\UnittestLesson\\": "src/"
    }
  }
}